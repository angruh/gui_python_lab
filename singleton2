class Singleton:
    _instance = None
    def __new__(cls,):
        if cls._instance is None:
            cls._instance = object.__new__(cls)
        return cls._instance

    def __init__(self):
        self.value = 0


obj1 = Singleton()
print(obj1.value)

obj2 = Singleton()
obj2.value = 3
print(obj1.value)
